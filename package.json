{
  "name": "@mzhub/cortex",
  "version": "0.1.1",
  "description": "Persistent memory for AI agents - A tiered memory system with fact extraction and conflict resolution",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.mjs",
      "require": "./dist/providers/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.mjs",
      "require": "./dist/middleware/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "logo.png"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "memory",
    "llm",
    "agents",
    "knowledge-graph",
    "context",
    "openai",
    "anthropic",
    "gemini",
    "groq",
    "cerebras",
    "ai-memory",
    "chatbot-memory"
  ],
  "author": "MZ Hub",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MZHub-Labs/cortex.git"
  },
  "homepage": "https://github.com/MZHub-Labs/cortex#readme",
  "bugs": {
    "url": "https://github.com/MZHub-Labs/cortex/issues"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@cerebras/cerebras_cloud_sdk": "^1.11.0",
    "@google/generative-ai": "^0.21.0",
    "@types/node": "^22.13.1",
    "@types/uuid": "^10.0.0",
    "groq-sdk": "^0.15.0",
    "mongodb": "^6.13.0",
    "openai": "^4.82.0",
    "pg": "^8.13.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "uuid": "^11.0.3"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@cerebras/cerebras_cloud_sdk": "^1.11.0",
    "@google/generative-ai": ">=0.21.0",
    "groq-sdk": "^0.15.0",
    "mongodb": "^6.13.0",
    "openai": "^4.82.0",
    "pg": "^8.13.1"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    },
    "groq-sdk": {
      "optional": true
    },
    "@cerebras/cerebras_cloud_sdk": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}